{
  "semi": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "parser": "babel-ts",
  "singleAttributePerLine": true,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": [
        "*.json"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.scss"
      ],
      "options": {
        "parser": "scss"
      }
    }
  ]
}